{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": "model.description.missing"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "maxLength": 100,
      "required": true,
      "type": "string"
    },
    "user": {
      "unique": true,
      "model": "user",
      "plugin": "users-permissions"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "tags": {
      "required": true,
      "type": "json"
    },
    "description": {
      "maxLength": 5000,
      "required": true,
      "type": "richtext"
    }
  }
}